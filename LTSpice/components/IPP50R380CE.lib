.SUBCKT cool_500_i2_var dd g s Tj t1 PARAMS: act=1 dVth=0 dR=0 Inn=1u Unn=10 Rmax=1 Rs=1u dC=0 heat=1

*control parameter: 1 if diode should store charge, 0 otherwise
.PARAM enable_diode=1
     
.PARAM w0={0.1p+SQRT(act)*0.8p}   w1={16p*act} w2={19p*act}  x11=-1    x2=-75m  
.PARAM Uoff=0.25    y11={exp(Uoff*x11)}
.PARAM w3={400p*act}  x3=-500m 
.PARAM w4={60p*act}   x4=1
.PARAM w7={0.2p*act}  w8={300p*act}  x8=-60m                                
.PARAM Cgs={115p*act} Cox={w0+w1+w2}
.PARAM k14=-2   deltb=1
.PARAM f4={18.0p*0.4}       f5=7p       sl=0.1p        f3=110p     f3a=0p
.PARAM  ps1={240p}     ps2=-0.2      ps5=60f       ps6=6.2p
.PARAM  qs1=2.0p    qs2=1.226n  qs3=-0.1667        qs4=3.7p      qs5=-6.25m
.PARAM  pc11=3      pc21=1.2f   pc22=80
.PARAM  qc80=0.656n qc81=40p   qc82=79p      qc83=125p      qc84=-0.04

.PARAM L=2u            fpar1=0.1       fpar2=0.9619        nmu3=0.78375
.PARAM Vth0=3.7        fpar29=273           fpar30=298        fpar3=3.7m
.PARAM fpar4=500m       fpar5=100p        fpar6=550         fpar17=0.1
.PARAM fpar7=0         fpar9=4
.PARAM nmu=3.15        tt=450n
.PARAM fpar12=1.1          fpar13={1.23/act}  fpar14={42m/act}   fpar15=13      fpar16=-4m
.PARAM fpar18=85.8u       fpar19=-29        fpar20=1.08         fpar21=0.262p
.PARAM fpar22={0.000003077*act/L}
 
.PARAM Vmin=2.95     Vmax=4.85         dCmax=0.33     
.PARAM Vth={Vth0+(Vmax-Vth0)*limit(dVth,0,1)-(Vmin-Vth0)*limit(dVth,-1,0)}
.PARAM p1={Unn-Inn*Rs-Vth0}
.PARAM p2={(((p1-SQRT(p1**2-4*fpar2/fpar22*Inn*(1+fpar1*p1)))/fpar2*fpar12+1)**2-1)/(4*fpar12)}
.PARAM p3={fpar15/(2*(Inn*(Inn*(Rmax-Rs)-p2)))}
.PARAM Rlim={p3*(-fpar15+SQRT(fpar15**2+4*(Rmax*Inn)**2-8*Rmax*Inn*(Inn*Rs+p2)+(2*p2+2*Inn*Rs)**2))}
.PARAM dRd={fpar13+if(dVth==0,limit(dR,0,1)*max(Rlim-fpar13,0),0)}
.PARAM  dC1={1+dCmax*limit(dC,0,1)}
.PARAM  y0={f5*act*dC1}
.PARAM  x0={(f4-f5)/sl}  x1={f4/sl} dx={x1-x0} y1={(ps5*act+ps6)*dC1}
.PARAM  y2={pc21*act*dC1}
.PARAM  Cox1={ps1*act*dC1}
.PARAM  C0={qs1*act*dC1}
.PARAM  C1={qs2*act*dC1}
.PARAM  C2={qs4*act*dC1}
.PARAM  C3={qc83*act*dC1}
.PARAM  Cgs0={(f3a+f3*act)*dC1}
.PARAM  aqc81={act*qc81*dC1}
.PARAM  a0={(qc80-qc81)/qc82} a1={qc80/qc82} da={a1-a0}


.FUNC fpar13(fpar29)  {(fpar17*dRd+(1-fpar17)*dRd*(fpar29/fpar30)**nmu)}
.FUNC CF(fpar29,Iepi) {(fpar15**2)/max(1,fpar15**2-(fpar13(fpar29)*Iepi)**limit(2+fpar16*(fpar29-fpar30),1.2,3))}
.FUNC QCdg1(x) {y0*min(x,x1)+y1*max(x-x1,0)+y2/2*max(0, x-pc22)**2+(y0-y1)*((limit(x,x0,x1)-x0)**3/(dx*dx)*((limit(x,x0,x1)-x0)/(2*dx)-1))}
.FUNC QCds1(x) {aqc81*min(x,a1)+aqc81*((limit(x,a0,a1)-a0)**3/(da*da)*((limit(x,a0,a1)-a0)/(2*da)-1))}


.FUNC  fpar24(Uee,p,pp,z1)  {if(Uee>pp,(Uee-fpar2*z1)*z1,p*(pp-p)/fpar2*exp((Uee-pp)/p))}
.FUNC  fpar25(Uds,p,Uee,z1,Tjx) {(fpar22/(1+fpar1*Uee)*(fpar29/Tjx)**nmu3)*fpar24(Uee,p,min(2*p,p+fpar2*Uds),z1)}
.FUNC  fpar28(Uds,Ugs,Tjx,p)  {fpar25(Uds,p,Ugs-Vth+fpar3*(Tjx-fpar29),min(Uds,(Ugs-Vth+fpar3*(Tjx-fpar29))/(2*fpar2)),Tjx)}
.FUNC  fpar26(Uds,Tjx)      {act*exp(min(fpar19+(Uds-fpar6-fpar4*(Tjx-fpar29))/fpar7,23))}
.FUNC  fpar27(Uds,Ugs,Tjx) {sgn(Uds)*fpar28((SQRT(1+4*fpar12*abs(Uds))-1)/2/fpar12,Ugs,Tjx,fpar9*fpar18*Tjx)}

R1 g s 1G
G_G1 d s VALUE={fpar27(V(d,s),V(g,s),fpar30+limit(V(Tj),-200,999))}
Rd01 d s 500Meg

V_Ichannel d1 d 0
V_Iepi     dd d2 0  
G_G_Rd     d2 d1 VALUE {V(d2,d1)/(fpar13(fpar30+LIMIT(V(t1),-200,999))*CF(fpar30+LIMIT(V(t1),-200,999),abs(I(V_Iepi))))}
R_R_ERd_g  d2 d1  10k

G_Rdiod    dd    dio2 VALUE {V(dd,dio2)/(fpar14*((fpar30+LIMIT(V(t1),-200,999))/fpar29)**1.4)}
R_Rdiod    dd    dio 500Meg
V_Idiode   dio2  dio 0

*body diode does not have dynamic temperature behaviour in order to improve commutation convergence
Dbody      s    di    DBODY
.MODEL DBODY  D(IS={fpar21*act}  N={fpar20}  RS=2u  EG=1.12  TT={tt})

Rd02       s    di 500Meg
G_diode2   s    di VALUE={-fpar26(V(di,s),fpar30+LIMIT(V(t1),-200,999))}
V_Idio     dio  di 0
E_Edg1    d1   ox  VALUE {V(d1,g)-(exp(ps2*max(V(d1,g)+pc11,0))-exp(ps2*max(0,pc11)))/ps2-min(-pc11,V(d1,g))-max(0,pc11)}
C_Cdg1   ox   g   {Cox1}
E_Edg4    d1   ox4 VALUE {V(d1,g)-QCdg1(V(d1,g))/y0}
C_Cdg4  ox4   g   {y0} 

C_Cds0    d1      s  {C0}
E_Eds1    d1  edep1  VALUE {if(V(d1,s)>0,V(d1,s)-(exp(qs3*max(V(d1,s),0))-1)/qs3,0)}
C_Cds1    edep1   s   {C1}
E_Eds2    d1   edep2 VALUE {if(V(d1,s)>0,V(d1,s)-(exp(qs5*max(V(d1,s),0))-1)/qs5,0)}
C_Cds2    edep2   s   {C2}
E_Eds3    d1   edep3 VALUE {if(V(d1,s)>0,V(d1,s)-(exp(qc84*max(V(d1,s),0))-1)/qc84,0)}
C_Cds3    edep3   s   {C3}
E_Eds4    d1   edep4 VALUE {V(d1,s)-QCds1(V(d1,s))/aqc81}
C_Cds4    edep4   s  {aqc81}

C_Cgs   g    s   {Cgs0}

G_G_Ptot_channel 0 Tj VALUE {heat*LIMIT(V(d,s)*I(V_Ichannel),0,100k) }
G_G_Ptot_Epi     0 t1 VALUE {heat*(LIMIT(V(dd,d1)*I(V_Iepi),0,100k)+LIMIT(V(dd,dio)*I(V_Idiode)+V(dio,s)*I(V_Idio),0,100k))}

.ENDS
